{
  "name": "webdevetc/laraveluniqueidetc",
  "keywords": [
    "laravel",
    "webdevetc",
    "trait",
    "package",
    "eloquent",
    "model",
    "unique",
    "id"
  ],
  "description": "Easily add a unique Id field to your Laravel eloquent models. Every time you create a new eloquent model in Laravel that uses this trait, it will automatically set a unique id column",
  "license": "MIT",
  "support": {
    "docs": "https://webdevetc.com/"
  },
  "authors": [
    {
      "name": "WebDevEtc",
      "homepage": "https://webdevetc.com/",
      "role": "developer",
      "email": "packages@webdevetc.com"
    }
  ],
  "require": {
    "illuminate/database": "~5.5.0|~5.6.0|5.7.*",
    "illuminate/support": "~5.5.0|~5.6.0|5.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "WebDevEtc\\LaravelUniqueIdEtc\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WebDevEtc\\LaravelUniqueIdEtc\\LaravelUniqueIdEtcServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
